<OptionScreen>
    name: 'Options'
    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'bottom'
        Image:
            source: 'res/b/Lobby_bg.png'
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                size_hint: (1, 0.1)
                MenuBtn:
                    text: lang.localize("TERM_BACK").upper()
                    size_hint_x: 0.8
                    on_release: 
                        app.sm.current = 'Main'
                    size_hint_y: 1
                Button:
                    text: lang.localize("TERM_SAVE").upper()
                    font_name: 'res/texgyreheros-bolditalic.otf'
                    size_hint_x: 0.2
                    outline_width: 2
                    background_color: (255, 0, 0, 0.2)
                    background_normal: ''
                    size_hint_y: 1
                    on_release: 
                        self.background_color = (255, 0, 0, 0.2)
