        BoxLayout:
            size_hint: (0.2, 0.1)
            Label:
                text: cfg.img_credit(root)
                id: credit
                outline_width: 2
                markup: True
                font_name: 'res/texgyreheros-bolditalic.otf'
                font_size: 12
                on_ref_press: webbrowser.open(root.artLink)
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'

<LogScreen>
    nameLabel: nameLabel
    rateLabel: rateLabel
    timeLabel: timeLabel
    resultList: resultList
    logList: logList
    name: 'Log'
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        Image:
            source: 'res/b/Lobby_bg.png'
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                size_hint: (1, 0.1)
                MenuBtn:
                    size_hint_x: 0.8
                    size_hint_y: 1
                    text: lang.localize("TERM_BACK").upper()
                    on_release: 
                        app.sm.current = 'Main'
                Button:
                    text: 'COPY'
                    font_name: 'res/texgyreheros-bolditalic.otf'
                    size_hint_x: 0.2
                    size_hint_y: 1
                    outline_width: 2
                    background_color: (255, 0, 0, 0.2)
                    background_normal: ''
                    on_release: 
                        self.background_color = (255, 0, 0, 0.2)
                        root.copySummary()
                    on_press: 
                        self.background_color = (100, 0, 0, 0.9)  
            TabbedPanel:
                size_hint_y: 0.9
                do_default_tab: False
                background_color: (0,0,0,0)
                tab_width: self.parent.width / 2
                TabbedPanelItem:
                    text: 'Summary'
                    outline_width: 2
                    background_color: '#343B88'
                    BoxLayout:
                        orientation: 'vertical'
                        BoxLayout:
                            size_hint_y: 0.4
                            orientation: 'vertical'
                            Label:
                                id: nameLabel
                                outline_width: 2
                                font_name: 'res/notosansjpbold.otf'
                                font_size: 20
                                text: 'Player vs. Player'
                            Label:
                                id: rateLabel
                                outline_width: 2
                                font_name: 'res/notosansjpbold.otf'
                                font_size: 16
                                text: 'XX% Winrate, #W/#L (# Total)'
                            Label:
                                id: timeLabel
                                outline_width: 2
                                font_name: 'res/notosansjpbold.otf'
                                font_size: 16
                                text: '#Hours Played, Avg. Match Time #:#'
                        BoxLayout:
                            size_hint_y: 0.6
                            orientation: 'vertical'
                            DummyBtn:
                                size_hint_y: 0.22
                                id: spreadLabel
                                text: 'Results Spread'
                            ScrollView:
                                size_hint_y: 0.78
                                do_scroll_x: False  
                                do_scroll_y: True
                                BoxLayout:
                                    id: resultList
                                    orientation: 'vertical'
                                    size_hint_y: None
                                    height: self.minimum_height
                TabbedPanelItem:
                    text: 'Battle Log'
                    outline_width: 2
                    background_color: '#343B88'
                    BoxLayout:
                        orientation: 'vertical'
                        BattleLog:
                            id: logList
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'

<LogOnlyScreen>
    logList: logList
    name: 'LogOnly'
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        Image:
            source: 'res/b/Lobby_bg.png'
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                size_hint: (1, 0.1)
                MenuBtn:
                    size_hint_x: 1
                    size_hint_y: 1
                    text: lang.localize("TERM_BACK").upper()
                    on_release: 
                        app.sm.current = 'Main'
            BattleLog:
                id: logList
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'

