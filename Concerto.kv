                    on_press: 
                        self.background_color = (100, 0, 0, 0.9)  
            BoxLayout:
                size_hint_x: 1
                size_hint_y: 0.080
                Button:
                    text: 'Game Config'
                    font_name: 'res/texgyreheros-bolditalic.otf'
                    outline_width: 2
                    background_color: (135/255,195/255,255/255, 0.8)
                    on_release:
                        root.input()
                Button:
                    text: 'Controller Config'
                    font_name: 'res/texgyreheros-bolditalic.otf'
                    outline_width: 2
                    background_color: (135/255,195/255,255/255, 0.8)
                    on_release:
                        root.dinput()
                Button:
                    text: 'Palette Editor'  
                    font_name: 'res/texgyreheros-bolditalic.otf' 
                    outline_width: 2
                    background_color: (135/255,195/255,255/255, 0.8)
                    on_release:
                        root.paledit()
            ScrollView:
                size_hint: (1,0.820)
                do_scroll_x: False
                do_scroll_y: True
                GridLayout:
                    id: opt_grid
                    cols: 1
                    size_hint_y: None
                    height: self.minimum_height
                    
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'

<DirectModal>:
    size_hint: None,None
    size: 400,105
    auto_dismiss: False
    join_ip: join_ip
    connect_txt: connect_txt
    GridLayout:
        padding: 20,20
        spacing: [0,4]
        cols: 1
        BoxLayout:
            Label:
                text: '%s: ' % lang.localize("TERM_CONNECTIP")
                font_name: 'res/texgyreheros-bolditalic.otf'
                outline_width: 1
                size_hint_x: 0.3
            TextInput:
                multiline: False
                id: join_ip
                size_hint_x: 0.5
            Button:
                id: connect_txt
                text: lang.localize("TERM_JOIN")
                font_name: 'res/texgyreheros-bolditalic.otf'
                outline_width: 2
                size_hint_x: 0.2
                on_release:
                    root.screen.join()
        Button:
            text: lang.localize("TERM_CLOSE")
            font_name: 'res/texgyreheros-bolditalic.otf'
            outline_width: 2
            on_release: 
                root.dismiss()

<GameModal>:
    modal_txt: modal_txt
    close_btn: close_btn
    btn_row: btn_row
    size_hint: None,None
    size: 400,200
    auto_dismiss: False 
    GridLayout:
        padding: [30,15]
        cols: 1
        GridLayout:
            padding: [0,5]
            cols: 1
            Label:
                id: modal_txt
                halign: 'center'
                valign: 'top'
                size: self.texture_size
                text_size: self.size
                size_hint_y: 0.60
            GridLayout:
                cols: 2
                size_hint_y: 0.20
                id: btn_row
                Button:
                    id: close_btn
                    font_name: 'res/texgyreheros-bolditalic.otf'
                    outline_width: 2
                    
<ProgressModal>:
    modal_txt: modal_txt
    prog_bar: prog_bar
    size_hint: None,None
    size: 400,100
    auto_dismiss: False 
    background_color: '#343B88'
    BoxLayout:
        padding: 20,20
        orientation: 'vertical'
        Label:
            id: modal_txt
            background_color: (0, 0, 0, 0)
            text_size: self.size
            halign: 'center'
            valign: 'top'
        ProgressBar:
            id: prog_bar

<SessionModal>:
    modal_txt: modal_txt
    btn_1: btn_1
    btn_2: btn_2
    size_hint: None,None
    size: 550,350
    auto_dismiss: False 
    BoxLayout:
        padding: 20,20
        orientation: 'vertical'
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            Label:
                size_hint_y: None
                height: self.texture_size[1]
