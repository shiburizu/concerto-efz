#:import webbrowser webbrowser
#:import threading threading
#:import res kivy.resources
#:import pyperclip pyperclip
#:import cfg config
#:import lang ui.lang

<MenuBtn>
    text_size: self.size
    halign: 'left'
    valign: 'middle'
    size_hint_y: None
    height: 36
    padding_x: 50
    background_normal: 'res/menubtn.png'
    outline_width: 2
    on_press: self.background_color = (135/255,195/255,255/255, 1)
    on_release: self.background_color = (1, 1, 1, 1)

<SessionBtn>
    size_hint_y: None
    height: 32
    padding_x: 10
    text_size: self.size
    halign: 'left'
    valign: 'middle'
    font_name: 'res/notosansjpbold.otf'
    background_normal: 'res/menubtn.png'
    background_down: 'res/menubtn.png'
    outline_width: 2
    on_press: self.background_color = (0,0,0,1)
    on_release: self.background_color = (0,0,0,1)
    
<PlayerRow>
    size_hint_y: None
    height: 32
    canvas.before:
        Color:
            rgba: (0,0,0,0.2)
        Rectangle:
            pos: self.pos
            size: self.size
    GridLayout:
        rows: 1
        Button:
            size_hint_x: 0.8
            id: PlayerBtn
            outline_width: 2
            font_name: 'res/notosansjpbold.otf'
            background_color: (0, 0, 0, 0)
            on_press: self.background_color = (135/255,195/255,255/255, 1)
            on_release: self.background_color = (0, 0, 0, 0)
            text_size: self.size
            halign: 'left'
            valign: 'center'
            padding_x: 50
        Button:
            size_hint_x: 0.2
            id: WatchBtn
            outline_width: 2
            font_name: 'res/texgyreheros-bolditalic.otf' 
            background_color: (0, 0, 0, 0)
            on_press: self.background_color = (135/255,195/255,255/255, 1)
            on_release: self.background_color = (0, 0, 0, 0)
            text_size: self.size
            halign: 'right'
            valign: 'center'
            padding_x: 10

<DummyBtn>
    text_size: self.size
    halign: 'center'
    valign: 'middle'
    size_hint_y: None
    height: 32
    padding_x: 50
    outline_width: 2
    font_name: 'res/texgyreheros-bolditalic.otf'
    background_normal: ''
    background_color: (0,0,0,0.3)

<LobbyBtn>:
    text: 'LOBBY'
    size_hint_x: 0.25
    size_hint_y: None
    height: 36
    outline_width: 2
    font_name: 'res/texgyreheros-bolditalic.otf'
    background_color: (255, 0, 0, 0.2)
    background_normal: ''
    on_release: 
        self.background_color = (255, 0, 0, 0.2)
    on_press: 
        self.background_color = (100, 0, 0, 0.9)  

<SpinnerOption>:
    text_size: self.size
    halign: 'center'
    valign: 'middle'
    background_color: '#210205'
    height: 28
    background_normal: ''

<Spinner>:
    text_size: self.size
    halign: 'center'
    valign: 'middle'
    background_color: '#343B88'

<MainScreen>:
    artIndex: cfg.art_choice()
    name: 'Main'
    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'bottom'
        Image:
            source: cfg.find_bg_img(root)
        Image:
            source: 'res/gradient.png'
        Label:
            size_hint: (0.975,1.875)
            text_size: self.size
            halign: 'left'
            valign: 'center'
            text: lang.localize("HEADER_MAIN")
            font_name: 'res/texgyreheros-bolditalic.otf'
            font_size: 20
            outline_width: 2
        BoxLayout:
            orientation: 'vertical'
            GridLayout:
                size_hint: (0.6183, 1)
                cols: 1
                padding: (0,50)
                spacing: [0,5]
                MenuBtn:
                    text: '1. %s' % lang.localize("MAIN_MENU_ONLINE")
                    on_release: app.sm.current = 'Online'
                MenuBtn:
                    text: '2. %s' % lang.localize("MAIN_MENU_LOCAL_VS")
                    on_release: app.MainScreen.local()
                MenuBtn:
                    text: '3. %s' % lang.localize("MAIN_MENU_TOURNAMENT_VS")
                    on_release: app.MainScreen.tournament()
                MenuBtn:
                    text: '4. %s' % lang.localize("MAIN_MENU_RESOURCES")
                    on_release: app.sm.current = 'Resources'
                MenuBtn:
                    text: '5. %s' % lang.localize("MAIN_MENU_OPTIONS")
                    on_release: app.OptionScreen.load()
                MenuBtn:
                    text: '6. %s' % lang.localize("MAIN_MENU_ABOUT")
                    on_release: app.sm.current = 'About'
                MenuBtn:
                    text: '7. %s' % lang.localize("MAIN_MENU_UPDATE")
                    on_release: root.update()
        Image:
            source: cfg.find_char_img(root)
            id: char
        Label:
            id: version
            size_hint: (0.97,0.075)
            text_size: self.size
            halign: 'left'
            valign: 'center'
            text: 'Version'
            font_name: 'res/texgyreheros-bolditalic.otf'
            font_size: 12
            outline_width: 2
        BoxLayout:
            size_hint: (0.2, 0.075)
            Label:
                text: cfg.img_credit(root)
                id: credit
                outline_width: 2
                markup: True
                font_name: 'res/texgyreheros-bolditalic.otf'
                font_size: 12
                on_ref_press: webbrowser.open(root.artLink)
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'bottom'
        BoxLayout:
            padding: 0,30
            DummyBtn:
                id: welcome
                text: lang.localize("MAIN_MENU_WELCOME")

<AboutScreen>
    name: 'About'
    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'bottom'
        Image:
            source: 'res/b/Lobby_bg.png'
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                size_hint_y: 0.1
                MenuBtn:
                    text: lang.localize("TERM_BACK").upper()
                    on_release: 
                        app.sm.current = 'Main'
                    size_hint_y: 1
            BoxLayout:
                size_hint_y: 0.9
                padding: 20,5
                Label:
                    id: about
                    size_hint: (1,1)
                    text_size: self.size
                    halign: 'left'
                    valign: 'top'
                    markup: True
        Label:
            id: version
            size_hint: (0.97,0.1)
            text_size: self.size
            halign: 'left'
            valign: 'center'
            text: 'Version'
            font_name: 'res/texgyreheros-bolditalic.otf'
            font_size: 12
            outline_width: 2
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'

<LobbyScreen>:
    player_list: player_list
    challenge_list: challenge_list
    match_list: match_list
    lobby_code: lobby_code
    name: 'Lobby'
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        Image:
            source: 'res/b/Lobby_bg.png'
        BoxLayout:
            orientation: 'vertical'
            GridLayout:
                rows: 1
                size_hint: (1, 0.1)
                MenuBtn:
                    size_hint_x: 0.348
                    padding_x: 25
                    text: lang.localize("TERM_LEAVE").upper()
                    on_release: 
                        root.exit()
                DummyBtn:
                    size_hint_x: 0.58
                    id: lobby_code
                    text: 'Lobby ID: 1234'
                    padding_x: 15
                    text_size: self.size
                    halign: 'right'
                    valign: 'middle'
                    background_color: (0,0,0,0)
                    on_release: root.invite_link()
            ScrollView:
                size_hint: (1,0.9)
                do_scroll_x: False
                do_scroll_y: True
                GridLayout:
                    height: self.minimum_height
                    size_hint_y: None
                    size_hint_x: 1
                    cols: 1
                    spacing: [0,2]
                    GridLayout:
                        id: challenge_list
                        cols: 1
                        height: self.minimum_height
                        size_hint_x: 1
                        size_hint_y: None
                        spacing: [0,2]
                    GridLayout:
                        id: player_list
                        cols: 1
                        height: self.minimum_height
                        size_hint_x: 1
                        size_hint_y: None
                        spacing: [0,2]
                    GridLayout:
                        id: match_list
                        cols: 1
                        height: self.minimum_height
                        size_hint_x: 1
                        size_hint_y: None
                        spacing: [0,2]    
    AnchorLayout
        id: lobbyAnchor
        anchor_x: 'center'
        anchor_y: 'top'
        LobbyBtn:
            text: lang.localize("LOBBY_MENU_MAINMENU")
            on_release: app.sm.current = 'Main'
                        
<LobbyList>:
    lobby_code: lobby_code
    lobby_type: lobby_type
    lobby_view: lobby_view
    name: 'LobbyList'
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        Image:
            source: 'res/b/Lobby_bg.png'
        Image:
            source: 'res/lowergradient.png'
        BoxLayout:
            orientation: 'vertical'
            Label:
                size_hint_y: 0.1
                text: lang.localize("LOBBY_LIST_HEADER")
                font_name: 'res/texgyreheros-bolditalic.otf'
                outline_width: 1
            RecycleView:
                size_hint: (1,0.7)
                do_scroll_x: False
                do_scroll_y: True
                GridLayout:
                    size_hint: (1,0.7)
                    id: lobby_view
                    cols: 1
                    height: self.minimum_height
                    size_hint_y: None
                    spacing: [0,5]
            GridLayout:
                cols: 2
                size_hint_y: 0.2
                GridLayout:
                    rows: 2
                    padding: [20,5]
                    spacing: [0,5]
                    GridLayout:
                        size_hint_y: None
                        height: 32
                        cols: 3
                        Label:
                            text_size: self.size
                            outline_width: 1
                            text: '%s: ' % lang.localize('TERM_LOBBYCODE')
                            halign: 'right'
                            valign: 'middle'
                        TextInput:
                            id: lobby_code
                            multiline: False
                            foreground_color: (255, 255, 255, 1)
                            background_color: (255, 0, 0, 0.2)
                        Button:
                            text: lang.localize('TERM_JOIN')
                            outline_width: 1
                            background_color: '#343B88'
                            on_release: root.join()
                    GridLayout:
                        size_hint_y: None
                        height: 32
                        cols: 3
                        Label:
                            text_size: self.size
                            text: '%s: ' % lang.localize("TERM_TYPE")
                            halign: 'right'
                            valign: 'middle'
                            outline_width: 1
                        Spinner:
                            id: lobby_type
                            text: 'Private'
                            values: 'Private','Public'
                            outline_width: 1
                            background_color: '#343B88'
                        Button:
                            text: lang.localize('TERM_CREATE')
                            background_color: '#343B88'
                            outline_width: 1
                            on_release: root.create()
                GridLayout:
                    rows: 2
                    padding: [20,5]
                    spacing: [0,5]
                    Button:
                        text: lang.localize('TERM_REFRESH')
                        outline_width: 1
                        background_color: '#343B88'
                        size_hint_y: None
                        height: 32
                        halign: 'right'
                        padding_x: 10
                        text_size: self.size
                        halign: 'left'
                        valign: 'middle'
                        on_release: 
                            root.refresh()
                    Button:
                        text: lang.localize('TERM_BACK')
                        outline_width: 1
                        background_color: '#343B88'
                        size_hint_y: None
                        height: 32
                        halign: 'right'
                        padding_x: 10
                        text_size: self.size
                        halign: 'left'
                        valign: 'middle'
                        on_release: 
                            app.sm.current = 'Online'

<OnlineScreen>:
    name: 'Online'
    artIndex: cfg.art_choice()
    AnchorLayout:
        anchor_x: 'right'
        anchor_y: 'bottom'
        Image:
            source: cfg.find_bg_img(root)
        Image:
            source: 'res/gradient.png'
        Label:
            size_hint: (0.975,1.875)
            text_size: self.size
            halign: 'left'
            valign: 'center'
            text: lang.localize("HEADER_ONLINE")
            font_name: 'res/texgyreheros-bolditalic.otf'
            font_size: 20
            outline_width: 2
        BoxLayout:
            orientation: 'vertical'
            GridLayout: 
                size_hint_x: 0.6183
                cols: 1
                padding: (0,50)
                spacing: [0,5]
                MenuBtn:
                    text: '1. %s' % lang.localize("ONLINE_MENU_HOST")
                    on_release:
                        root.host()
                MenuBtn:
                    text: '2. %s' % lang.localize("ONLINE_MENU_JOIN")
                    on_release:
                        root.direct()
                MenuBtn:
                    text: '3. %s' % lang.localize("ONLINE_MENU_SPECTATE")
                    on_release: 
                        root.direct(True)
                MenuBtn:
                    text: '4. %s' % lang.localize("ONLINE_MENU_LOBBY")
                    on_release: 
                        root.lobby()
                MenuBtn:
                    text: '5. ???'
                MenuBtn:
                    text: '0. %s' % lang.localize("TERM_BACK").upper()
                    on_release: app.sm.current = 'Main'
        Image:
            source: cfg.find_char_img(root)
